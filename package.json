{
  "name": "git-sweep",
  "version": "0.3.0",
  "description": "Clean up remote branches for git repo",
  "main": "lib/sweep.js",
  "keywords": [
    "git",
    "clean",
    "remote"
  ],
  "bin": {
    "git-sweep": "bin/git-sweep"
  },
  "bugs": {
    "url": "https://github.com/pswai/git-sweep/issues"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "dependencies": {
    "babel-polyfill": "^6.22.0",
    "commander": "^2.9.0",
    "moment": "^2.17.1",
    "nodegit": "^0.16.0",
    "readline-sync": "^1.4.6"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-jest": "^18.0.0",
    "babel-preset-env": "^1.1.8",
    "jest": "^19.0.2",
    "rimraf": "^2.5.4"
  },
  "engines": {
    "node": ">= 4"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pswai/git-sweep.git"
  },
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watchAll",
    "build": "./node_modules/.bin/babel src --out-dir lib --ignore __test__/**,__mocks__/**",
    "prepublish": "./node_modules/.bin/rimraf lib && npm run build"
