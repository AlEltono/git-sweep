    "minimist": "^1.2.0",
    "moment": "^2.12.0",
    "nodegit": "^0.11.9"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-3": "^6.5.0",
    "rimraf": "^2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pswai/git-sweep.git"
  },
  "scripts": {
    "build": "$(npm bin)/babel src --out-dir lib",
